<h1>Order list</h1>
<div class="row">
  <div class="col-md-3">
    <div class="list-group">
      {{#each model as |order|}}
        {{#link-to 'orders.order' order.id class="list-group-item" }}
          Order: {{order.id}}
        {{/link-to}}
      {{/each}}
    </div>
  </div>
  <div class="col-md-9">
    {{outlet}}
  </div>
</div>
